{
  "title": "laravel-auditing",
  "gitbook": ">3.0.0",
  "plugins": ["edit-link", "theme-laravel-auditing", "-fontsettings", "github", "replace"],
  "structure": {
    "summary": "documentation.md"
  },
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/owen-it/laravel-auditing-doc/edit/master/",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/owen-it/laravel-auditing"
    },
    "replace": {
      "substitutes": [
        {"pattern": "Summary", "flags": "g", "substitute": "Sumario"} 
      ]
    }
  },
  "links": {
    "sharing": {
      "facebook": false,
      "twitter": false
    }
  }
}